{"ast":null,"code":"import { Swiper, SwiperSlide } from 'swiper/vue';\nimport { FreeMode, Pagination } from 'swiper';\n// Import Swiper styles\nimport 'swiper/css';\nimport 'swiper/css/bundle';\nexport default {\n  __name: 'Home',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n\n    // npm 라이브러리\n    const SwiperProjectList = {\n      slidesPerView: 'auto',\n      spaceBetween: 14,\n      freeMode: true,\n      // centeredSlides: true,\n      pagination: true,\n      modules: [FreeMode, Pagination],\n      breakpoints: {\n        // 1200px 이상일 때\n        1300: {\n          slidesPerView: 4.8,\n          spaceBetween: 16,\n          freeMode: false\n          // pagination:true,\n        },\n\n        // 1200px 이상일 때\n        1000: {\n          slidesPerView: 3.8,\n          spaceBetween: 16,\n          freeMode: false\n          // pagination:true,\n        }\n      }\n    };\n\n    function slider() {\n      var winW = window.innerWidth; //화면 가로사이즈\n      var swiper = undefined;\n      var viewNum = ''; //슬라이드 개수 (옵션)\n      var loopChk = ''; //무한반복 체크\n      var slideNum = $('.slider .swiper-slide').length; //슬라이드 총 개수\n      var slideInx = 0; //현재 슬라이드 index\n\n      //디바이스 체크\n      var winWChk = '';\n      $(window).on('load resize', function () {\n        winW = window.innerWidth;\n        if (winWChk != 'mo' && winW <= 768) {\n          //모바일 버전으로 전환할 때\n          winWChk = 'mo';\n          setTimeout(function () {\n            sliderAct();\n          }, 300);\n        }\n        if (winWChk != 'pc' && winW >= 769) {\n          //PC 버전으로 전환할 때\n          winWChk = 'pc';\n          setTimeout(function () {\n            sliderAct();\n          }, 300);\n        }\n      });\n      function sliderAct() {\n        //슬라이드 초기화 \n        if (swiper != undefined) {\n          swiper.destroy();\n          swiper = undefined;\n        }\n\n        //slidesPerView 옵션 설정\n        if (winW > 768) {\n          //PC 버전\n          viewNum = 4;\n        } else {\n          //mobile 버전\n          viewNum = 2;\n        }\n\n        //loop 옵션 체크\n        if (slideNum > viewNum) {\n          loopChk = true;\n        } else {\n          loopChk = false;\n        }\n        swiper = new Swiper(\".slider .inner\", {\n          slidesPerView: \"auto\",\n          initialSlide: slideInx,\n          loop: loopChk,\n          centeredSlides: true,\n          navigation: {\n            nextEl: $('.slider .swiper-next'),\n            prevEl: $('.slider .swiper-prev')\n          },\n          on: {\n            activeIndexChange: function () {\n              slideInx = this.realIndex; //현재 슬라이드 index 갱신\n            }\n          }\n        });\n      }\n    }\n\n    const __returned__ = {\n      SwiperProjectList,\n      slider,\n      get Swiper() {\n        return Swiper;\n      },\n      get SwiperSlide() {\n        return SwiperSlide;\n      },\n      get FreeMode() {\n        return FreeMode;\n      },\n      get Pagination() {\n        return Pagination;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["Swiper","SwiperSlide","FreeMode","Pagination","SwiperProjectList","slidesPerView","spaceBetween","freeMode","pagination","modules","breakpoints","slider","winW","window","innerWidth","swiper","undefined","viewNum","loopChk","slideNum","$","length","slideInx","winWChk","on","setTimeout","sliderAct","destroy","initialSlide","loop","centeredSlides","navigation","nextEl","prevEl","activeIndexChange","realIndex"],"sources":["/Users/ellen/Desktop/작업/와우스/wows/src/pages/Home.vue"],"sourcesContent":["<template>\n\t<header @mouseover=\"onMouseSubmenu\" @mouseout=\"onMouseOutSubmenu\">\n\t\t<div class=\"c_inner c_flex_wrap\">\n\t\t\t<h1 class=\"logo\">\n\t\t\t\t<router-link to=\"/\">\n\t\t\t\t\t<img src=\"@/assets/images/logo.svg\" alt=\"와우스\" class=\"image-contain img\" />\n\t\t\t\t</router-lin